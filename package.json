{
  "name": "acsys",
  "private": true,
  "scripts": {
    "build": "react-scripts build && (if exist server\\build rmdir /s /q server\\build) && move build server",
    "build-linux": "react-scripts build && rm -rf server/build && mv build server",
    "dev": "concurrently \"react-scripts start\" \"nodemon -r dotenv/config server/server.js\"",
    "docker-build": "docker build . --file Dockerfile --tag acsysio/acsys ",
    "docker-run": "docker run -p 8080:8080 acsysio/acsys",
    "docker-run-env": "docker run -p 8080:8080 --env-file .env acsysio/acsys",
    "lint": "./node_modules/.bin/eslint app.js",
    "react": "react-scripts start",
    "start": "node server/server.js",
    "format": "prettier --write **/*.js",
    "test-react": "react-scripts test --env=jsdom"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.1.1",
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.5.1",
    "bcrypt": "5.0.0",
    "body-parser": "^1.17.2",
    "clsx": "^1.1.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.4",
    "express-jwt": "^6.0.0",
    "firebase-admin": "9.4.2",
    "immutability-helper": "^3.0.1",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.27",
    "moment": "^2.24.0",
    "morgan": "^1.8.2",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.6.2",
    "react": "17.0.1",
    "react-datetime": "3.1.1",
    "react-dnd": "11.1.3",
    "react-dnd-html5-backend": "11.1.3",
    "react-dom": "17.0.1",
    "react-quill": "^2.0.0-beta.4",
    "react-router-dom": "^5.1.2",
    "react-scripts": "4.0.1",
    "sqlite3": "5.0.9",
    "temp": "^0.9.1"
  },
  "devDependencies": {
    "concurrently": "5.3.0",
    "debug": "^4.3.4",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "nodemon": "2.0.6",
    "prettier": "^2.7.1"
  },
  "proxy": "http://localhost:8080"
}
