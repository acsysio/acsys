{
  "name": "acsys",
  "private": true,
  "scripts": {
    "build": "react-scripts build && (if exist server\\build rmdir /s /q server\\build) && move build server",
    "build-linux": "react-scripts build && rm -rf server/build && mv build server",
    "dev": "concurrently \"react-scripts start\" \"nodemon -r dotenv/config server/server.js\"",
    "docker-build": "docker build . --file Dockerfile --tag acsysio/acsys ",
    "docker-run": "docker run -p 8080:8080 acsysio/acsys",
    "docker-run-env": "docker run -p 8080:8080 --env-file .env acsysio/acsys",
    "lint": "./node_modules/.bin/eslint app.js",
    "react": "react-scripts start",
    "start": "node server/server.js",
    "format": "prettier --write **/*.js",
    "test-react": "react-scripts test --env=jsdom"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.2.3",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "clsx": "^1.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-fileupload": "^1.1.7-alpha.4",
    "express-jwt": "^7.7.5",
    "firebase-admin": "11.0.1",
    "immutability-helper": "^3.0.1",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.35",
    "moment": "^2.24.0",
    "morgan": "^1.8.2",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.7",
    "react": "17.0.1",
    "react-datetime": "3.1.1",
    "react-dnd": "11.1.3",
    "react-dnd-html5-backend": "11.1.3",
    "react-dom": "17.0.1",
    "react-quill": "^2.0.0-beta.4",
    "react-router-dom": "^5.1.2",
    "react-scripts": "5.0.1",
    "sqlite3": "5.0.10",
    "temp": "^0.9.1"
  },
  "devDependencies": {
    "concurrently": "7.3.0",
    "debug": "^4.3.4",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "2.0.19",
    "prettier": "^2.7.1"
  },
  "proxy": "http://localhost:8080"
}
